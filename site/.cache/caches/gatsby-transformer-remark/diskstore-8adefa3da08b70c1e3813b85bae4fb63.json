ion":{"start":{"line":15,"column":4,"offset":1513},"end":{"line":15,"column":73,"offset":1582},"indent":[]}}],"position":{"start":{"line":15,"column":1,"offset":1510},"end":{"line":15,"column":73,"offset":1582},"indent":[]}},{"type":"listItem","spread":false,"checked":null,"children":[{"type":"paragraph","children":[{"type":"text","value":"They’re DUAL! Because we have two people per desk in our room, I can rack this light in-between them and cut cost in half!","position":{"start":{"line":16,"column":4,"offset":1586},"end":{"line":16,"column":126,"offset":1708},"indent":[]}}],"position":{"start":{"line":16,"column":4,"offset":1586},"end":{"line":16,"column":126,"offset":1708},"indent":[]}}],"position":{"start":{"line":16,"column":1,"offset":1583},"end":{"line":16,"column":126,"offset":1708},"indent":[]}},{"type":"listItem","spread":false,"checked":null,"children":[{"type":"paragraph","children":[{"type":"text","value":"They’re rack-mountable. No more falling off the desk and breaking lamps.","position":{"start":{"line":17,"column":4,"offset":1712},"end":{"line":17,"column":76,"offset":1784},"indent":[]}}],"position":{"start":{"line":17,"column":4,"offset":1712},"end":{"line":17,"column":76,"offset":1784},"indent":[]}}],"position":{"start":{"line":17,"column":1,"offset":1709},"end":{"line":17,"column":76,"offset":1784},"indent":[]}},{"type":"listItem","spread":false,"checked":null,"children":[{"type":"paragraph","children":[{"type":"text","value":"It’s durable. We’re talking Middle Atlantic, the company who makes road-ready, metal equipment. This is going to outlast any Target light.","position":{"start":{"line":18,"column":4,"offset":1788},"end":{"line":18,"column":142,"offset":1926},"indent":[]}}],"position":{"start":{"line":18,"column":4,"offset":1788},"end":{"line":18,"column":142,"offset":1926},"indent":[]}}],"position":{"start":{"line":18,"column":1,"offset":1785},"end":{"line":18,"column":142,"offset":1926},"indent":[]}},{"type":"listItem","spread":false,"checked":null,"children":[{"type":"paragraph","children":[{"type":"text","value":"Only ","position":{"start":{"line":19,"column":4,"offset":1930},"end":{"line":19,"column":9,"offset":1935},"indent":[]}},{"type":"text","value":"$","position":{"start":{"line":19,"column":9,"offset":1935},"end":{"line":19,"column":11,"offset":1937},"indent":[]}},{"type":"text","value":"50! That’s as much or less than I would have paid for one “cheap” LED desk lamp.","position":{"start":{"line":19,"column":11,"offset":1937},"end":{"line":19,"column":91,"offset":2017},"indent":[]}}],"position":{"start":{"line":19,"column":4,"offset":1930},"end":{"line":19,"column":91,"offset":2017},"indent":[]}}],"position":{"start":{"line":19,"column":1,"offset":1927},"end":{"line":19,"column":91,"offset":2017},"indent":[]}}],"position":{"start":{"line":14,"column":1,"offset":1436},"end":{"line":19,"column":91,"offset":2017},"indent":[1,1,1,1,1]}},{"type":"paragraph","children":[{"type":"text","value":"So I bought a few. They are awesome. I’m going to be testing them out for a while and I’ll update this post if any of them fail, but I’m not expecting that to happen for a few years.","position":{"start":{"line":21,"column":1,"offset":2019},"end":{"line":21,"column":183,"offset":2201},"indent":[]}}],"position":{"start":{"line":21,"column":1,"offset":2019},"end":{"line":21,"column":183,"offset":2201},"indent":[]}},{"type":"definition","identifier":"1","label":"1","title":null,"url":"http://www.middleatlantic.com/power/lt1r.htm","position":{"start":{"line":23,"column":1,"offset":2203},"end":{"line":23,"column":50,"offset":2252},"indent":[]}}],"position":{"start":{"line":1,"column":1,"offset":0},"end":{"line":24,"column":1,"offset":2253}}}}